{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "hosting": {
      "host": "localhost",
      "port": "8082"
    },
    "functions": {
      "port": 5002
    },
    "database": {
      "port": 9001
    },
    "ui": {
      "enabled": true
    }
  }
}
